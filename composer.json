{
    "name": "akira/filament-auth-logs",
    "description": "A filament package to log and notify authentication",
    "keywords": [
        "Akira",
        "laravel",
        "filament-auth-logs"
    ],
    "homepage": "https://github.com/akira/filament-auth-logs",
    "support": {
        "issues": "https://github.com/akira/filament-auth-logs/issues",
        "source": "https://github.com/akira/filament-auth-logs"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "kidiatoliny",
            "email": "kidiatoliny@akira-io.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Akira\\FilamentAuthLogs\\": "src/",
            "Akira\\FilamentAuthLogs\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akira\\FilamentAuthLogs\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akira\\FilamentAuthLogs\\FilamentAuthLogsServiceProvider"
            ],
            "aliases": {
                "FilamentAuthLogs": "Akira\\FilamentAuthLogs\\Facades\\FilamentAuthLogs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}